{
  "name": "lichi/vk-grab-contents",
  "type": "library",
  "description": "Grab contents From Vk",
  "keywords": ["Lichi","lichi", "vk-grab-content", "vk grab content", "vk grabber"],
  "homepage": "https://github.com/IvanHunters/lichi-vk-grab-contents",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Okhotnikov",
      "email": "ohotnikovivan@yandex.ru"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "ext-curl": "*",
    "ext-json": "*",
    "lichi/vk-sdk": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Lichi\\Grab\\": "src/",
      "Lichi\\Grab\\Post\\": "src/grab"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
