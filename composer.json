{
  "name": "lichi/vk-sdk",
  "description": "Lichi VK SDK",
  "authors": [
    {
      "name": "Ivan Okhotnikov",
      "email": "ohotnikov.ivan@e-queo.com"
    }
  ],
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "commercial",
  "require": {
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Lichi\\Vk\\": "src/",
      "Lichi\\Vk\\Sdk\\": "src/sdk"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "vimeo/psalm": "^4.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}
